V2 <- function(y) {
v <- rep(0, length(y) / 2)
for (t in seq(4, length(y), by = 2)) {
s <- 0
for (i in seq(2, t / 2, by = 2)) {
s <- s + mean(y[1:i])
}
v[t/2] <- t * (s / (t/2) - mean(y[1:t])) ^2
}
return(v)
}
